<launch>
    <!-- Start goal_sending node -->
    <node name="goal_sending" pkg="assistant" type="goal_sending" output="screen"> 
    </node>

    <!-- Start WebSocket client node -->
    <node name="websocket_client" pkg="assistant" type="websocket_client" output="screen">
        <param name="host" value="172.30.1.31"/>
        <param name="port" value="81"/>
    </node>
</launch>